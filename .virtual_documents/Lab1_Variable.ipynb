def GCD(a,b):
    while b:
        a,b=b,a%b
    return a


GCD(0,75)


# recursive
def GCD_r(a,b):
    if b==0:
        return a
    else:
        return GCD_r(b,a%b)


GCD_r(0,75)





def flip_dict(dic):
    out={}
    for key,val in dic.items():
        if val not in out:
            out[val]=[]
        out[val].append(key)
    return out


