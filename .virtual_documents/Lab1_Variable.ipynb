def is_cyclone_phrase(phrase):
    words = phrase.split(' ')
    res = True
    j=0
    while j< len(words) and res :
        cycle = [ i//2 if i %2==0 else -((i+1)//2) for i in range(len(words[j]))]
        word_cycle=[words[j][l] for l in cycle]
        if word_cycle != sorted(word_cycle):
            res = False
        j=j+1
    return res 
