    j=0
    while j< len(words) and res :
        cycle = [ words[j][i//2] if i %2==0 else words[j][-((i+1)//2)] for i in range(len(words[j]))]
        k = 0
        while k< len(words[j])-1 and cycle[k]<= cycle[k+1]:
            k+=1
        if k < len(words[j])-1:
            res = False
        j=j+1
    return res 


